{% extends 'portal/layout/index.html.twig' %}
{% block portal_content %}
    <section class="banner-sec">
        <div class="content thank-you" data-aos="fade-right" data-aos-delay="100">
            <div class="icon-head" data-aos="fade-right" data-aos-delay="200"><img
                        src="../resources/images/common/icons/icon-submitted.png">
            </div>
            <h3 data-aos="fade-right" data-aos-delay="200">Thank You!</h3>
            <h5 data-aos="fade-right" data-aos-delay="200">For registering your project with Giving Circle. Your request has been submitted successfully.</h5>
            <p data-aos="fade-right" data-aos-delay="200">Your project will be live once it has been reviewed by our team.
                Please check the <a href="{{ path('my-accounts-projects') }}">My Account</a> section for updates.</p>
            <a href="{{ path('homepage') }}" class="btn btn-primary">Back to Home</a>
        </div>
    </section>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
<script>
    $(document).ready(function () {
        $('body').addClass('inner-pg');
        $('body').addClass('create-element');
        $('body').addClass('logged');
    });
</script>
{% endblock %}