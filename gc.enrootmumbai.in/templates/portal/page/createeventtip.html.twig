{% extends 'portal/layout/index.html.twig' %}
{% block portal_content %}
    <!--Main Wrapper Starts-->
    <div class="wrapper">
        <section>
            <div class="container">
                {% if(pageContentByPage is empty) %}
                <div class="row">
                    <h4>No Active Page Found...</h4>
                </div>
                {% else %}
                <div class="row">
                    <div class="col">
                        <nav aria-label="breadcrumb" data-aos="fade-right" data-aos-delay="500"
                             class="breadcrumb d-flex justify-content-end">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="{{ path('homepage') }}"><i class="fa fc-home"></i></a>
                                </li>
                                <li class="breadcrumb-item active" aria-current="page">{{ pageContentByPage.pageTitle }}</li>
                            </ol>
                        </nav>
                    </div>
                </div>
                <h2 data-aos="fad e-right" data-aos-delay="200">{{ pageContentByPage.pageTitle }}</h2>
                <div class="white-box" data-aos="fade-right" data-aos-delay="200">
                    {# Content Start from Cms  #}
                    {% for content in pageContentByPage.cmsPageContent %}
                        {{ content|raw }}
                    {% endfor %}
                    {# Content End from Cms  #}
                </div>
                {% endif %}
            </div>
        </section>
    </div>

    <!--Main Wrapper End-->
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script>
        $(document).ready(function () {
            $('body').removeClass('inner-pg');
            $('body').addClass('static');
        });
    </script>
{% endblock %}